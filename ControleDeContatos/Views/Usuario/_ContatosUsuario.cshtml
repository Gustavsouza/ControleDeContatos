@model List<ContatoModel>

<table id="table-contatos-usuario" class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nome</th>
            <th scope="col">Email</th>
            <th scope="col">Celular</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Any())
        {
            @foreach (var contato in Model)
            {
                <tr>
                    <th scope="row">@contato.id</th>
                    <td>@contato.nome</td>
                    <td>@contato.email</td>
                    <td>@contato.celular</td>
                </tr>
            }
        }
        else
        {
            <tr class="odd"><td valign="top" colspan="4">Nenhum contato encontrado</td></tr>
        }

    </tbody>
</table>



